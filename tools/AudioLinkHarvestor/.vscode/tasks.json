{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build-debug",
            "type": "process",
            "command": "dotnet",
            "args": ["build","${workspaceFolder}/AudioLinkHarvestor.csproj"],
            "problemMatcher": "$msCompile",
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "build-release",
            "type": "process",
            "command": "dotnet",
            "args": ["build","${workspaceFolder}/AudioLinkHarvestor.csproj", "-c", "Release"],
            "problemMatcher": "$msCompile"
        }
    ]
}